<% layout('layout') %>
<h2>Şarkı Yükle</h2>
<form action="/upload" method="POST" enctype="multipart/form-data">
    <input type="text" name="title" placeholder="Şarkı Başlığı" required>
    <input type="text" name="artist" placeholder="Sanatçı Adı" required>
    <input type="file" name="songFile" accept="audio/*" required>
    <button type="submit">Yükle</button>
</form>
<a href="/dashboard">Geri</a>
